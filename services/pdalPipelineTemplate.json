{
    "pipeline": [
       {
          "type": "readers.ept",
          
          "filename": "{{eptFilename}}",
          "bounds": "{{bounds}}"
       },
       {
            "type":"filters.transformation",
            "matrix":"{{matrixTransformation}}"

        },
        {
            "type":"filters.reprojection",
            "in_srs":"EPSG:4978",
            "out_srs":"EPSG:2154"
        },
        {
            "type":"filters.crop",
            "polygon":"POLYGON(({{polygon}}))"
        },
        "output.las"
    ]
}